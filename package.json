{
  "name": "ee979-chat",
  "version": "1.0.1",
  "main": "electron/main.js",
  "description": "chat app for ee979 service",
  "author": "Jon Ye",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "ELECTRON_START_URL=http://192.168.2.102:3001 electron .",
    "pack": "rm -rf dist && build",
    "publish": "rm -rf dist && build --win -p always"
  },
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^3.1.2",
    "js-sha1": "^0.6.0",
    "mqtt": "^2.18.8",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.4",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.0",
    "styled-components": "^3.4.10"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "build": {
    "appId": "com.github.superdorkman.ee979chatapp",
    "extends": null,
    "files": [
      "build/**/*",
      "electron/*"
    ],
    "productName": "易易在线",
    "mac": {
      "category": "your.app.category.type",
      "icon": "public/assets/images/logo.png",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "icon": "public/assets/icons/icon.ico"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "superdorkman",
        "repo": "ee979-chat"
      }
    ]
  }
}
